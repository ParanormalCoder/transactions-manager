{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "lib": ["es2017", "es2018", "dom"],
    "outDir": "./dist",
    "rootDir": "./server",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["**/node_modules"],
  "include": ["server/**/*.ts"]
}
